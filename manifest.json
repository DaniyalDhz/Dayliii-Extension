{
    "manifest_version": 2,
    "name": "new",
    "description": "This extension helps...",
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
    "version": "0.1.0",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "scripts/audio_file.wav", "reminder.html"
    ],
    "permissions": [
        "storage",
        "identity",
        "identity.email",
        "tabs",
        "http://127.0.0.1:5000/test",
        "http://127.0.0.1:5000/get/info",
        "http://127.0.0.1:5000/test",
        "http://0.0.0.0:5500/index.html",
        "http://0.0.0.0:5000/test",
        "http://127.0.0.1:5000/Time"
    ],
    "background": {
        "scripts": ["jquery-3.5.1.min.js", "background.js", "flipclock-min.js", "script.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["https://www.blank.org/", "http://localhost:5500/*", "<all_urls>"],
        "js": ["jquery-3.5.1.min.js", "flipclock-min.js", "content.js"],
        "css": ["normalize.min.css", "styles2.css", "flipclock.css", "css.css"]
    }]
}